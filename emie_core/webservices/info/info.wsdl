<!-- begin  kinds-->
    <message name="listUnitKindsRequest">
        <part name="parameters" element="tns:listUnitKindsRequest"/>
    </message>
    <message name="listUnitKindsResponse">
        <part name="parameters" element="tns:listUnitKindsResponse"/>
    </message>
    <portType name="listUnitKindsPortType">
        <operation name="listUnitKindsOper">
            <documentation>Операция получения списка видов учреждений</documentation>
            <input wsam:Action="urn:listUnitKindsRequest" message="tns:listUnitKindsRequest"/>
