                    </xsd:element>
                    <xsd:element name="areas" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation><xsd:documentation>Список идентификаторов территорий учреждений</xsd:documentation></xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="listUnitsResponse">
