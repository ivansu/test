                    </xsd:element>
                    <xsd:element name="id" type="xsd:integer">
                        <xsd:annotation><xsd:documentation>Идентификатор вида</xsd:documentation></xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
<!-- end  kinds-->

<!-- begin units-->
            <xsd:element name="listUnitsRequest" type="tns:listUnitsRequest">
