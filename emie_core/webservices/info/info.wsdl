                    </xsd:element>
                    <xsd:element name="eServiceResult" type="tns:EServiceResult" minOccurs="0" >
                        <xsd:annotation><xsd:documentation>Заголовок ответа eServiceHeader</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="returns" type="tns:unit" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation><xsd:documentation>Список объектов "Учреждение"</xsd:documentation></xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="unit">
                <xsd:annotation><xsd:documentation>Описание объекта "Учреждение"</xsd:documentation></xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="name" type="xsd:string">
                        <xsd:annotation><xsd:documentation>Наименование</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="id" type="xsd:integer">
                        <xsd:annotation><xsd:documentation>Идентификатор</xsd:documentation></xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>



            <xsd:element name="unitsInfoRequest" type="tns:unitsInfoRequest">
                <xsd:annotation><xsd:documentation>Запрос информации об учреждениях</xsd:documentation></xsd:annotation>
            </xsd:element>

            <xsd:element name="unitsInfoResponse" type="tns:unitsInfoResponse">
                <xsd:annotation><xsd:documentation>Ответ на запрос информации об учреждениях</xsd:documentation></xsd:annotation>
            </xsd:element>

            <xsd:complexType name="unitsInfoRequest">
                <xsd:annotation><xsd:documentation>Параметры запроса информации об учреждениях</xsd:documentation></xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="requestHeader" type="tns:RequestHeader" minOccurs="0" >
                        <xsd:annotation><xsd:documentation>Заголовок запроса</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="eServiceHeader" type="tns:EServiceHeader" minOccurs="0" >
                        <xsd:annotation><xsd:documentation>Заголовок запроса eServiceHeader</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="units" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation><xsd:documentation>Список идентификаторов учреждений по которым необходима информация</xsd:documentation></xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="unitsInfoResponse">
                <xsd:annotation><xsd:documentation>Параметры ответа на запрос информации об учреждениях</xsd:documentation></xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="responseHeader" type="tns:ResponseHeader" minOccurs="0" >
                        <xsd:annotation><xsd:documentation>Заголовок ответа</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="eServiceResult" type="tns:EServiceResult" minOccurs="0" >
                        <xsd:annotation><xsd:documentation>Заголовок ответа eServiceHeader</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="returns" type="tns:unitInfo" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation><xsd:documentation>Список объектов "Информация об учреждении"</xsd:documentation></xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="unitInfo">
                <xsd:annotation><xsd:documentation>Описание объекта "Информация об учреждении"</xsd:documentation></xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="id" type="xsd:string">
                        <xsd:annotation><xsd:documentation>Идентификатор</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="name" type="xsd:string">
                        <xsd:annotation><xsd:documentation>Наименование</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="address" type="xsd:string">
                        <xsd:annotation><xsd:documentation>Адрес</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="telephone" type="xsd:string">
                        <xsd:annotation><xsd:documentation>Телефон</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="email" type="xsd:string">
                        <xsd:annotation><xsd:documentation>Email</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="web_page" type="xsd:string">
                        <xsd:annotation><xsd:documentation>Адрес сайта в интернете</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="director" type="xsd:string">
                        <xsd:annotation><xsd:documentation>Директор</xsd:documentation></xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>



            <xsd:element name="unitDetailsRequest" type="tns:unitDetailsRequest">
                <xsd:annotation><xsd:documentation>Запрос расширенной информации об учреждении</xsd:documentation></xsd:annotation>
            </xsd:element>

