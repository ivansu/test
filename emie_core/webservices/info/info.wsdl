                        <xsd:annotation><xsd:documentation>Заголовок запроса eServiceHeader</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="unit" type="xsd:string">
                        <xsd:annotation><xsd:documentation>Идентификатор учреждения по которым необходима расширенная информация</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="format" type="xsd:string" minOccurs="0">
                        <xsd:annotation><xsd:documentation>Формат возвращаемой информации (html, json, по умолчанию html)</xsd:documentation></xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="unitDetailsResponse">
                <xsd:annotation><xsd:documentation>Параметры ответа на запрос расширенной информации об учреждении</xsd:documentation></xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="responseHeader" type="tns:ResponseHeader" minOccurs="0" >
                        <xsd:annotation><xsd:documentation>Заголовок ответа</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element name="eServiceResult" type="tns:EServiceResult" minOccurs="0" >
                        <xsd:annotation><xsd:documentation>Заголовок ответа eServiceHeader</xsd:documentation></xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" nillable="true" name="returns" type="xsd:string">
                        <xsd:annotation><xsd:documentation>Страница расширенной информации об учреждении</xsd:documentation></xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
<!-- end  units-->
